{
    "title": "Key: (double tap) | single tap",
    "rules": [
        {
            "description": "f: backspace | f",
            "manipulators": [
              {
                "type": "basic",
                "from": {
                  "key_code": "f",
                  "modifiers": {
                    "optional": [
                      "any"
                    ]
                  }
                },
                "to": [
                  {
                    "key_code": "delete_or_backspace"
                  }
                ],
                "to_after_key_up": [
                  {
                    "set_variable": {
                      "name": "f pressed",
                      "value": 0
                    }
                  }
                ],
                "conditions": [
                  {
                    "type": "variable_if",
                    "name": "f pressed",
                    "value": 1
                  }
                ]
              },
              {
                "type": "basic",
                "parameters": {
                  "basic.to_delayed_action_delay_milliseconds": 160
                },
                "from": {
                  "key_code": "f",
                  "modifiers": {
                    "optional": [
                      "any"
                    ]
                  }
                },
                "to": [
                  {
                    "set_variable": {
                      "name": "f pressed",
                      "value": 1
                    }
                  }
                ],
                "to_delayed_action": {
                  "to_if_invoked": [
                    {
                      "set_variable": {
                        "name": "f pressed",
                        "value": 0
                      }
                    },
                    {
                      "key_code": "f"
                    }
                  ]
                }
              }
            ]
          }
    ]
}